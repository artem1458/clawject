{
  "name": "@clawject/clawject-debug",
  "version": "0.0.1",
  "type": "commonjs",
  "scripts": {
    "run:upgradeClawject": "yarn upgrade clawject clawject-language-service && ts-patch install",
    "run:tsc": "tsc",
    "run:tscw": "tsc -w",
    "run:ttsc_debug": "cross-env CLAWJECT_DEBUG_MODE=true tsc",
    "run:ttsc_watch": "cross-env CLAWJECT_DEBUG_MODE=true tsc -w --project tsconfig.json",
    "run:node_debug": "node dist/src/index.js",
    "run:webpack_hot": "webpack serve --config webpack.config.ts --mode development",
    "run:webpack_prod": "webpack --config webpack.config.ts --mode production",
    "run:language_service": "tsserver --logVerbosity verbose --traceToConsole"
  },
  "dependencies": {
    "clawject": "../../dist/packages/clawject",
    "clawject-language-service": "../../packages/clawject-language-service"
  },
  "devDependencies": {
    "@types/webpack": "^5.28.1",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.5.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "ttypescript": "^1.5.15",
    "typescript": "^5.2.2",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
